@{
Layout = null;
}

<head>
    <link rel="preconnect" href="https://fonts.gstatic.com">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css">
    <link href="https://fonts.googleapis.com/css2?family=Poppins:wght@300;500;600&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.7.2/font/bootstrap-icons.css">
    <link href="~/css/inicio_sesion.css" rel="stylesheet" />

</head>

<body>
    <form method="post">
        <div class="text-center">
            <a href="/"><i class="bi bi-caret-left-fill"></i>Regresar</a>
        </div>
        <br />
        <div class="login-content">
            <img src="~/Img/favicon.png" />
        </div>
        <br />
        <label for="username">Usuario</label>
        <input placeholder="Ingrese su Usuario" type="text" required>
        <label for="password">Contraseña</label>
        <input placeholder="Ingrese su Contraseña" type="password" required>
        <br />
        @*@if (ViewData["Message"] != null)
        {
        <div class="alert alert-danger" role="alert" style="text-align:center">
            @ViewData["Message"]
        </div>
        }*@
        <button type="submit" class="submit" id="btnEntrar">Log In</button>
    </form>
</body>